  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Lisk.app",
        "build": "xcodebuild -UseModernBuildSystem=NO -project ios/Lisk.xcodeproj -scheme Lisk -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/Lisk.app",
        "build": "xcodebuild -UseModernBuildSystem=NO -project ios/Lisk.xcodeproj -scheme Lisk -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    }
