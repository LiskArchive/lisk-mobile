  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "ios-dev": "npx react-native run-ios --scheme LiskDev",
    "start": "npx react-native start",
    "release:android": "cd android && ./gradlew bundleRelease",
    "test": "cp env.test.json env.json && jest --config ./jest.config.js --env=jsdom",
    "test:live": "npm test -- --watch",
    "test:android": "cd android && ./gradlew --project-cache-dir ../.gradlecache test",
    "lint": "npx eslint --quiet ./",
    "i18n": "node ./scripts/scanner.config.js",
    "link": "./node_modules/.bin/rn-nodeify --hack --install",
    "postinstall": "patch-package && npx react-native setup-ios-permissions",
    "e2e:ios": "cp env.test.json env.json && detox build -c ios.debug && detox test -c ios.debug",
    "e2e:android": "cp env.test.json env.json && detox build -c android.debug && detox test -c android.debug",
    "prepare": "husky install"
  },
